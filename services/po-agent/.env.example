# PO Agent Service Environment Variables

# OpenAI API Key (required for LLM)
OPENAI_API_KEY=your_openai_api_key_here

# Tavily API Key (required for web search and extract)
TAVILY_API_KEY=your_tavily_api_key_here

# Service Configuration
HOST=0.0.0.0
PORT=8002
ENV=development

# LLM Configuration
LLM_MODEL=gpt-4o-mini
LLM_TEMPERATURE=0.7

# Logging
LOG_LEVEL=INFO
