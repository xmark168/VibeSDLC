{
  "name": "vibesdlc",
  "version": "1.0.0",
  "description": "VibeSDLC Microservices Platform",
  "private": true,
  "scripts": {
    "dev": "docker compose -f docker-compose.yml -f docker-compose.local.yml up",
    "dev:build": "docker compose -f docker-compose.yml -f docker-compose.local.yml up --build",
    "dev:down": "docker compose -f docker-compose.yml -f docker-compose.local.yml down",
    "prod": "docker compose up",
    "prod:build": "docker compose up --build",
    "prod:down": "docker compose down",
    "logs": "docker compose logs -f",
    "logs:management": "docker compose logs -f management-service",
    "logs:ai": "docker compose logs -f ai-agent-service",
    "logs:frontend": "docker compose logs -f frontend",
    "status": "docker compose ps",
    "clean": "docker compose down -v --remove-orphans"
  },
  "keywords": [
    "microservices",
    "fastapi",
    "react",
    "kafka",
    "docker"
  ],
  "author": "VibeSDLC Team",
  "license": "MIT"
}